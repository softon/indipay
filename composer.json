{
    "name": "softon/indipay",
    "description": "The Laravel 5 Package for Indian Payment Gateways. Currently supported gateways: CCAvenue, PayUMoney, EBS, CitrusPay, InstaMojo",
    "license": "MIT",
    "keywords": [
        "Laravel 5",
        "Indian Payment Gateways",
        "CCAvenue",
        "PayUMoney",
        "EBS",
        "CitrusPay",
        "InstaMojo"
    ],
    "authors": [
        {
            "name": "Shiburaj",
            "email": "powerupneo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~5.0|~6.0",
        "guzzlehttp/guzzle": "~5.0|~6.0"
    },
    "autoload": {
        "psr-4": {
            "Softon\\Indipay\\": "src/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Softon\\Indipay\\IndipayServiceProvider"
            ],
            "aliases": {
                "Indipay": "Softon\\Indipay\Facades\Indipay"
            }
        }
    }
}
